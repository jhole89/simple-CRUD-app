# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.ContactController.index
POST    /create                     controllers.ContactController.createContact
GET     /contacts                   controllers.ContactController.getContacts


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/assets", file: Asset)
